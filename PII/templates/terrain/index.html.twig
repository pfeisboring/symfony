{# {% extends 'base.html.twig' %} #}


{% block body %}
<style>
    .example-wrapper { margin: 1em auto; max-width: 800px; width: 95%; font: 18px/1.5 sans-serif; }
    .example-wrapper code { background: #F5F5F5; padding: 2px 6px; }
</style>

<div class="example-wrapper">
   <table>
       <thead>
           <th>Id Terrain</th>
           <th>Type Terrain</th>
           <th>Supprimer</th>
           <th>Modifier</th>
       </thead>
       <tbody>
       {% for terrain in b  %}
       <tr>
           <td>{{terrain.idTerrain}}</td>
           <td>{{terrain.typeTerrain}}</td>
           <td>
               <a href="{{ path('supp_terrain',{'id':terrain.idTerrain}) }}">Supprimer</a>
           </td>
           <td>
               <a href="{{ path('modifierTerrain',{'id':terrain.idTerrain}) }}">Modifier</a>
           </td>
       </tr>
       {% endfor %}
       </tbody>
   </table>
</div>
{% endblock %}
